    <decisions>
        <description>Sends case back to Bot if no Billing agent is available.</description>
        <name>Route_to_Billing_Agent</name>
        <label>Route to Billing Agent</label>
        <locationX>182</locationX>
        <locationY>458</locationY>
        <defaultConnectorLabel>No Billing agent available</defaultConnectorLabel>
        <rules>
            <name>Billing_Agent_Available</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>NumberofAvailableOnlineAgents</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Case_for_Billing_Agent</targetReference>
            </connector>
            <label>Billing Agent Available</label>
        </rules>
    </decisions>
