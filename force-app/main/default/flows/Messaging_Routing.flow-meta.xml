    <decisions>
        <description>Picks which messaging Queue based on Case Type</description>
        <name>Which_Messaging_Queue</name>
        <label>Which Messaging Queue</label>
        <locationX>666</locationX>
        <locationY>242</locationY>
        <defaultConnector>
            <targetReference>Fulfillment_Agent_Availability</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Route to Live Support</defaultConnectorLabel>
        <rules>
            <name>Case_Type_Billing</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Messaging_Session.Case_Record_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Billing</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Billing_Agent_Availability</targetReference>
            </connector>
            <label>Case Type Billing</label>
        </rules>
        <rules>
            <name>Case_Type_Sales</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Messaging_Session.Case_Record_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Sales</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Sales_Agent_Availability</targetReference>
            </connector>
            <label>Case Type Sales</label>
        </rules>
    </decisions>
