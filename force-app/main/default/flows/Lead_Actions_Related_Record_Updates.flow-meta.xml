    <description>Flow for all Actions and Related Record Updates</description>
    <environments>Default</environments>
    <formulas>
        <description>The text of the SMS message to send to Bark leads</description>
        <name>BarkSMSMessageText</name>
        <dataType>String</dataType>
        <expression>&quot;You recently requested assistance through Bark. We are excited to help! The first step is scheduling an appointment with our small business team. Use this link to find a time that works for you. https://1800accountant.com/sfapi/t/?w=&quot;+{!$Record.Id+&quot;&amp;c=SMS} Reply STOP to stop receiving messages.&quot;</expression>
    </formulas>
    <formulas>
        <name>BarkTSAInviteRecordName</name>
        <dataType>String</dataType>
        <expression>&quot;Bark TSA Invitation for &quot;+{!$Record.FirstName} + {!$Record.LastName}</expression>
    </formulas>
    <formulas>
        <description>Returns False only if Campaign(Last Touch) does not contain any of the following values - Entity Formation, Call, Chat, Join Now.</description>
        <name>CampaignLastTouchValues</name>
        <dataType>Boolean</dataType>
        <expression>OR(
CONTAINS(TEXT({!$Record.Campaign_Last_Touch__c}), &apos;Entity Formation&apos;),
CONTAINS(TEXT({!$Record.Campaign_Last_Touch__c}), &apos;Call&apos;),
CONTAINS(TEXT({!$Record.Campaign_Last_Touch__c}), &apos;Chat&apos;),
CONTAINS(TEXT({!$Record.Campaign_Last_Touch__c}), &apos;Join Now&apos;)
)</expression>
    </formulas>
    <formulas>
        <name>HourValueOfNowEST</name>
        <dataType>Number</dataType>
        <expression>VALUE(MID(TEXT(NOW()),12,2))-4</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>MVFGlobalMessageText</name>
        <dataType>String</dataType>
        <expression>&quot;You recently inquired about payroll services through Expert Market. We are excited to help! The first step is scheduling an appointment with our small business team. Use this link to find a time that works for you. https://1800accountant.com/sfapi/t/?w=&quot;+{!$Record.Id+&quot;&amp;c=SMS} Reply STOP to stop receiving messages.&quot;</expression>
    </formulas>
    <formulas>
        <name>MVFGlobalTSAInviteRecordName</name>
        <dataType>String</dataType>
        <expression>&quot;MVFGlobal TSA Invitation for &quot;+{!$Record.FirstName} + {!$Record.LastName}</expression>
    </formulas>
    <formulas>
        <name>Now</name>
        <dataType>DateTime</dataType>
        <expression>NOW()</expression>
    </formulas>
